{
    "eslint.enable": true,
    "eslint.problems.shortenToSingleLine": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "[javascript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
      "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "eslint.lintTask.enable": false,
    "editor.suggestSelection": "recentlyUsed",
  }
  